<%= month_calendar events: @hosted_space_bookings do |date, bookings| %>
  <%= content_tag 'div', class: 'calendarDay disabled-date', data: { calendardate: date } do %>
    <%= date.day %>
    <% bookings.each do |booking| %>
        <%= booking.user.first_name %>
    <% end %>
  <% end %>
<% end %>
